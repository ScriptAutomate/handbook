pany.mission","alias":"mission statement","position":{"start":{"line":130,"column":123,"offset":10133},"end":{"line":130,"column":169,"offset":10179},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"handbook.company.mission"}},{"type":"wiki","from":{"fname":"templates.people.onboarding","id":"c6b422a5-204a-41dd-8e47-f0b736160790","vaultName":"dendron.handbook"},"value":"handbook.company.key-principles","alias":"handbook.company.key-principles","position":{"start":{"line":131,"column":22,"offset":10201},"end":{"line":131,"column":57,"offset":10236},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"handbook.company.key-principles"}},{"type":"wiki","from":{"fname":"templates.people.onboarding","id":"c6b422a5-204a-41dd-8e47-f0b736160790","vaultName":"dendron.handbook"},"value":"dendron.sop.expensing","alias":"expense report process","position":{"start":{"line":160,"column":37,"offset":12430},"end":{"line":160,"column":85,"offset":12478},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"dendron.sop.expensing"}},{"type":"wiki","from":{"fname":"templates.people.onboarding","id":"c6b422a5-204a-41dd-8e47-f0b736160790","vaultName":"dendron.handbook"},"value":"handbook.people.onboarding.engineering","alias":"Engineering","position":{"start":{"line":188,"column":3,"offset":14296},"end":{"line":188,"column":57,"offset":14350},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"handbook.people.onboarding.engineering"}},{"type":"wiki","from":{"fname":"templates.people.onboarding","id":"c6b422a5-204a-41dd-8e47-f0b736160790","vaultName":"dendron.handbook"},"value":"handbook.people.onboarding.content","alias":"Content","position":{"start":{"line":189,"column":3,"offset":14353},"end":{"line":189,"column":49,"offset":14399},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"handbook.people.onboarding.content"}},{"type":"wiki","from":{"fname":"templates.people.onboarding","id":"c6b422a5-204a-41dd-8e47-f0b736160790","vaultName":"dendron.han