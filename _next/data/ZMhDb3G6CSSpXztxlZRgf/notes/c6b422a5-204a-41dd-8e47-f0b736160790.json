{"pageProps":{"note":{"id":"c6b422a5-204a-41dd-8e47-f0b736160790","title":"Onboarding","vault":{"fsPath":"handbook","name":"dendron.handbook"},"type":"note","desc":"","links":[{"type":"wiki","from":{"fname":"templates.people.onboarding","id":"c6b422a5-204a-41dd-8e47-f0b736160790","vaultName":"dendron.handbook"},"value":"handbook.company.values","alias":"passing it forward","position":{"start":{"line":39,"column":21,"offset":2690},"end":{"line":39,"column":110,"offset":2779},"indent":[]},"xvault":true,"sameFile":false,"to":{"fname":"handbook.company.values","anchorHeader":"pass-it-forward","vaultName":"dendron.handbook"}},{"type":"wiki","from":{"fname":"templates.people.onboarding","id":"c6b422a5-204a-41dd-8e47-f0b736160790","vaultName":"dendron.handbook"},"value":"handbook.sop.daily-journal","alias":"handbook.sop.daily-journal","position":{"start":{"line":52,"column":34,"offset":3706},"end":{"line":52,"column":64,"offset":3736},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"handbook.sop.daily-journal"}},{"type":"wiki","from":{"fname":"templates.people.onboarding","id":"c6b422a5-204a-41dd-8e47-f0b736160790","vaultName":"dendron.handbook"},"value":"schemas.user","alias":"here","position":{"start":{"line":53,"column":261,"offset":3997},"end":{"line":53,"column":295,"offset":4031},"indent":[]},"xvault":false,"sameFile":false,"to":{"fname":"schemas.user","anchorHeader":"dailyjournal"}},{"type":"wiki","from":{"fname":"templates.people.onboarding","id":"c6b422a5-204a-41dd-8e47-f0b736160790","vaultName":"dendron.handbook"},"value":"dendron.guides.tips","alias":"pinning","position":{"start":{"line":54,"column":52,"offset":4084},"end":{"line":54,"column":127,"offset":4159},"indent":[]},"xvault":true,"sameFile":false,"to":{"fname":"dendron.guides.tips","anchorHeader":"pinning-tabs","vaultName":"dendron.dendron-site"}},{"type":"wiki","from":{"fname":"templates.people.onboarding","id":"c6b422a5-204a-41dd-8e47-f0b736160790","vaultName":"dendron.handbook"},"value":"te